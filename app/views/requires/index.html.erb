

<h1>Requires</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @requires.each do |require| %>
      <tr>
        <td><%= link_to 'Show', require %></td>
        <td><%= link_to 'Edit', edit_require_path(require) %></td>
        <td><%= link_to 'Destroy', require, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Require', new_require_path %>
